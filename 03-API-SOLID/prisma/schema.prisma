generator client {
  provider = "prisma-client-js"
  output   = "../generated/prisma"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User {
  @@map("users")

  id        String   @id @default(uuid())
  name      String
  email     String   @unique
}

model CheckIn {
  @@map("check_ins")

  id        String   @id @default(uuid())
  create_at DateTime @default(now())
  validated_at DateTime?
}

model  Gym {
  @@map("gyms")

  id        String   @id @default(uuid())
  title String
  description String?
  phone String?
  latitude Decimal
  longitude Decimal
}